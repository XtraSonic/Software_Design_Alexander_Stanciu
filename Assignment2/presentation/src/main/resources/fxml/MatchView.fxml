<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" styleClass="root" stylesheets="@../styles/darkTheme.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="ro.utcluj.alexanderstanciu.sd.view.controllers.MatchViewController">
   <children>
      <VBox fx:id="vbox" alignment="CENTER" spacing="10.0" styleClass="root" stylesheets="@../styles/darkTheme.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" styleClass="root" stylesheets="@../styles/darkTheme.css">
               <children>
                  <Button mnemonicParsing="false" onAction="#returnToGame" onMousePressed="#returnToGame" text="Return to Game">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <Label fx:id="titleLabel" styleClass="root" stylesheets="@../styles/darkTheme.css" text="Label">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
            <Label fx:id="errorLabel" styleClass="error-label">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
